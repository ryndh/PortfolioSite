{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "react portfolio",
  "main": "react.js",
  "scripts": {
    "start": "webpack-dev-server --watch",
    "build": "webpack --mode production",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@emotion/babel-preset-css-prop": "^10.0.14",
    "@emotion/core": "^10.0.14",
    "css-loader": "^3.0.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "file-loader": "^4.0.0",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1"
  },
  "eslintConfig": {
    "extends": "react",
    "env": {
      "browser": true,
      "node": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "indent": [
        "error",
        2
      ]
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
