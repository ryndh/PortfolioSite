    "moduleNameMapper": {
      "\\.(jpg|png)$": "<rootDir>/src/mocks/fileMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
